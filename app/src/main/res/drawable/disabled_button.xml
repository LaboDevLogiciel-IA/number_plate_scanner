<?xml version = "1.0" encoding = "utf-8"?>
<selector
  xmlns:android = "http://schemas.android.com/apk/res/android"
  >
  <item>
    <shape android:shape = "rectangle">
      <corners android:radius = "32dip"/>
      <stroke
        android:color = "@color/neutral_5"
        android:width = "0dip"
        />
      <gradient
        android:startColor = "@color/neutral_5"
        android:endColor = "@color/neutral_5"
        android:angle = "-90"
        />
    </shape>
  </item>
</selector>
